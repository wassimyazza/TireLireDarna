# API URLs
VITE_DARNA_API_URL=http://localhost:4000
VITE_TIRELIRE_API_URL=http://localhost:5000
VITE_SSO_API_URL=http://localhost:4444

# SSO Configuration (PKCE OAuth)
VITE_SSO_CLIENT_ID=your-client-id
VITE_SSO_REDIRECT_URI=http://localhost:5173/auth/callback
VITE_SSO_AUTH_ENDPOINT=http://localhost:4444/auth/authorize
VITE_SSO_TOKEN_ENDPOINT=http://localhost:4444/auth/token
VITE_SSO_LOGOUT_ENDPOINT=http://localhost:4444/auth/logout

# Google OAuth (if using social login)
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Map provider (optional - Leaflet doesn't require key, but Mapbox does)
VITE_MAPBOX_TOKEN=pk.your-mapbox-token-here

# Stripe (client-side publishable key)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key

# MinIO / File Storage
VITE_MINIO_ENDPOINT=http://localhost:9000
VITE_MINIO_BUCKET=uploads

# WebSocket / Socket.IO
VITE_SOCKET_URL=http://localhost:4000

# Sentry (optional error tracking)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Environment
VITE_APP_ENV=development
